{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "src/main/resources/lib/util"
        ],
        "includePattern": "^(.*)\\.es$",
        "excludePattern": "^/test"
    },
    "opts" : {
        "destination": "./docs/"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}

