{
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "src/main/resources/lib/util/app.es",
            "src/main/resources/lib/util/content",
            "src/main/resources/lib/util/portal",
            "src/main/resources/lib/util/query",
            "src/main/resources/lib/util/value"
        ],
        "exclude": [
            "src/main/resources/lib/util/app.es"
        ],
        "includePattern": [
            ".+\\.es?$"
        ],
        "excludePattern": ".+\\.(test)\\.es"
    },
    "opts" : {
        "destination": "./docs/index.md",
        "recurse": true
    }
}

